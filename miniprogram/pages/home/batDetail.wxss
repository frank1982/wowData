page{
  position:relative;
  background: white;
}

#alertBg{
  
  width:100%;
  height:100%;
  background: rgba(0,0,0,0.7);
  position: fixed;
  z-index:109;
  top:0rpx;
  display: flex;
  flex-direction: column;
  
}
#box{

  width:550rpx;
  height:550rpx;
  background: white;
  margin-left: 100rpx;
  margin-top:250rpx;
  border-radius: 20rpx;
  display: flex;
  flex-direction: column;
  
}

#closeBtn{

  width:60rpx;
  height:60rpx;
  margin-top: 60rpx;
  margin-left: 345rpx;
}
#boxImg{
  position:fixed;
  width:180rpx;
  height:180rpx;
  left: 285rpx;
  top:270rpx;
  z-index:250;
}
#boxstr{

  width:550rpx;
  height:100rpx;
  text-align: center;
  margin-top:200rpx;
  line-height: 100rpx;
  font-size:46rpx;
}
#boxbtn{

  background: #051041;
  color:#d2b576;
  width:450rpx;
  height:80rpx;
  line-height: 80rpx;
  margin-left: 50rpx;
  margin-top:40rpx;
  text-align: center;
  border-radius: 6rpx;
  font-size: 42rpx;
}
#boxtxt{
  color:#696969;
  width:550rpx;
  height:50rpx;
  text-align: center;
  line-height: 50rpx;
  margin-left: 0rpx;
  margin-top:30rpx;
  font: 20rpx;
}



#btnArea{

  position: fixed;
  top:0rpx;
  width:100%;
  display: flex;
  flex-direction: row;
  z-index:99;
  background: white;
}
.btn{
  width:25%;
  height:120rpx;
  line-height: 120rpx;
  text-align: center;
  color: #696969;
}
#container{
  position: absolute;
  top:120rpx;
  z-index:10;
}
.oneView{
  width:750rpx;
  display: flex;
  position:relative;
  flex-direction: row;
  justify-content: flex-start;
  margin-top: 20rpx;
  align-items: center;
}
.noTxt{
  color:#d2b576;
  display: flex;
  justify-content: center;
  align-items: center;
  font-weight:bolder;
  height:100rpx;
  margin-left: 20rpx;
  width:100rpx;
  font-size:40rpx;
}
.serverName{

  height: 100rpx;
  line-height: 100rpx;
  color:#696969;
}
.rate{
  position: absolute;
  color:#d2b576;
  font-size:40rpx;
  height: 60rpx;
  line-height: 60rpx;
  right:40rpx;
  background: #051041;
  border-radius: 6rpx;
  width:120rpx;
  text-align: center;
}
.mycanvas{
  margin-top: 20rpx;
  width:650rpx;
  height:20rpx;
  margin-left: 50rpx;
}

.end{
  
  width:100%;
  height:200rpx;
  color:#696969;
  text-align:center;
  line-height:150rpx;
}
#bottomView{
  position: fixed;
  width: 100%;
  bottom:0rpx;
  display: flex;
  flex-direction: row;
  z-index: 99;
  align-items: center;
  background: white;
  border-top: 1px solid #DDDDDD; 
  box-shadow:0px 2px 4px 2px #DDDDDD;
}
.look{
  height: 100rpx;
  width:50%;
  flex-direction: row;
  display: flex;
  justify-content: center;
  align-items: center;
}
.icon{
  width:45rpx;
  height:45rpx;
}
.iconTxt{
  margin-left: 20rpx;
}
#txt0{
  color:#0079FF;
}
#txt1{
  color:#D50000;
}