  margin-left: 25rpx;
  width:160rpx;
}
.btnTitle{
  font-size:small;
}
.itemView{
  height:160rpx;
  width: 750rpx;
  border-radius:10rpx;
  margin-top: 3rpx;
  background-color: white;
}
.itemTopView{
  height:80rpx;
  display: flex;
  flex-direction:row;
  align-items: center;
  justify-content: flex-start;
  padding-left: 25rpx;
}
