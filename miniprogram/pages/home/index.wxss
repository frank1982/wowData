page {
  background: #f6f6f6;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}
#topView{
  background:#FFFFFF;
  height: 260rpx;
  width: 100%;
  display: flex;  
  justify-content: flex-start;
  flex-direction: column;
}
#bannerView{
  background-color: #051041;
  height:180rpx;
  width:700rpx;
  border-radius: 6rpx;
