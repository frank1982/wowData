  justify-content: flex-end;
  align-items: center;
  padding-right: 20rpx;
  margin-right: 25rpx;
}
#btnArea{
  height: 100rpx;
  width: 750rpx;
  display: flex;
  margin-top: 20rpx;
  background-color: white;
}
.btn{
  display: flex;
  justify-content: center;
