page {
  background: #F5F5F5;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}

#topView{
  background:#FFFFFF;
  height: 260rpx;
  width: 100%;
  display: flex;  
  justify-content: flex-start;
  flex-direction: column;
}
#bannerView{
  background-color: #051041;
  height:180rpx;
  width:700rpx;
  border-radius: 6rpx;
  display: flex;  
  justify-content: center;
  margin-left: 25rpx;
}
#iconView{
  height: 180rpx;
  width: 200rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  /*垂直水平居中*/
}
#iconImage{
  height: 100rpx;
  width: 100rpx;
}
#rightView{
  height: 180rpx;
  width: 500rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}
